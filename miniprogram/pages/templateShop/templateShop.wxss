/* pages/templateShop/templateShop.wxss */
.shopcar_container{background-color: #fff;margin-bottom: 102rpx;}
.shopcar_list{padding: 54rpx 0;border-bottom: 1px solid #F0F0F0;}
.list_content{position: relative;padding-left: 64rpx;}
.shopcar_img{width: 188rpx;height: 188rpx;border-radius: 16rpx;background-size: cover;background-position: center;background-repeat: no-repeat;float: left;}
.shopcar_info{float: right;width: 425rpx;}
.info_top .txt{font-size: 30rpx;color: #333333;line-height: 42rpx;width: 344rpx;}
.info_top .attr{font-size: 26rpx;color: #BCBCBC ;line-height: 36rpx;margin-top: 8rpx;}
.info_bottom{margin-top: 26rpx;}
.info_bottom .prime{font-size: 30rpx;color: #FF6248;line-height: 42rpx;float: left;}
.num_box{float: right;}
.num_box .btn{display: inline-block;color: #BCBCBC;font-size: 52rpx;line-height: 42rpx;vertical-align: top;width: 42rpx;height: 42rpx;position: relative;}
.num_box .btn::after,.num_box .btn::before{content: '';display: block;position: absolute;background-color: #BCBCBC;top: 0;bottom: 0;left: 0;right: 0;margin: auto;}
.num_box .btn::after{width: 28rpx;height: 4rpx;}
.num_box .btn::before{height: 28rpx;width: 4rpx;}
.num_box .btn.cut_btn::before{display: none;}
.num_box .num_change{display: inline-block;font-size: 26rpx;color: #333333;width: 40rpx;margin: 0 20rpx;line-height: 42rpx;text-align: center;vertical-align: top;}
.delete_btn{position: absolute;top: 10rpx;right: 0;width: 32rpx;height: 32rpx;background-size: cover;background-position: center;background-repeat: no-repeat;}
.list_content .select_radio{position: absolute;top: 0;bottom: 0;left: 0;margin: auto 0;width: 40rpx;height: 40rpx;background-image: url(data:image/png;base64,);background-size: cover;background-position: center;background-repeat: no-repeat;}
.remove .select_radio{background-image: url(data:image/png;base64,)}
.total_box{position: fixed;width: 100%;bottom: 0;left: 0;background-color: #F8F8F8;line-height: 68rpx;padding: 16rpx 0;z-index: 99;}
.select_all{font-size: 26rpx;color: #333333;float: left;padding-left: 60rpx;background-image: url(data:image/png;base64,);background-size: 40rpx 40rpx;background-position: left center;background-repeat: no-repeat;}
.select_all.all{background-image: url(data:image/png;base64,);}
.total_prime{float: right;}
.total_prime .prime{color: #FF6248;font-size: 30rpx;display: inline-block;}
.pay_btn{font-size: 34rpx;color: #fff;width: 160rpx;height: 68rpx;border-radius: 50rpx;text-align: center;display: inline-block;background-color: #FF6248;margin-left: 26rpx;}

/* 空购物车样式 */
.empty_img{margin: 0 auto;display: block;width: 578rpx;}
.empty_txt{color: #7E7E7E;font-size: 30rpx;line-height: 42rpx;margin: 14rpx 0 25rpx;text-align: center;}
.shop_btn{font-size: 34rpx;color: #FF6248;line-height: 68rpx;width: 218rpx;opacity: 0.96;border: 1px solid #FF6248;border-radius: 34rpx;margin: 0 auto;text-align: center;}


