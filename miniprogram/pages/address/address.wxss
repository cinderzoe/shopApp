/* pages/address/address.wxss */

.addr-form{background-color: #fff;}
.row{font-size: 13px;color: #333333;line-height: 36rpx;overflow: hidden;padding: 32rpx 0;border-top: 1px solid #F0F0F0;}
.row .txt{width: 25%;float: left;}
.row .write{width: 70%;float: right;text-align: right;}
.row .write.addr-write{width: 100%;text-align: left;}
.row .btn{display: inline-block;}
.row .btn-yes{position: relative;padding-right: 30rpx}
.row .btn-yes::after{content: '';display: block;position: absolute;width: 10rpx;height: 20rpx;background-image: url(data:image/png;base64,);background-size: cover;background-repeat: no-repeat;background-position: center;top: 50%;right: 0;margin-top: -10rpx;}
.row.default-btn{border-top: 20rpx solid rgba(245,245,245,1);background-color: #fff;}
.row.default-btn .write{position: relative;}
.row.default-btn .write::after{content: '';display: block;position: absolute;top: 0;right: 0;background-image: url(data:image/png;base64,);background-size: 40rpx 40rpx;background-position: left center;background-repeat: no-repeat;width: 40rpx;height: 40rpx;}
.row.default-btn.choice .write::after{background-image: url(data:image/png;base64,);}
.save-btn{font-size: 34rpx;color: #FFFFFF;text-align: center;background: #FF6248;border-radius: 50rpx;width: 430rpx;height: 80rpx;line-height: 80rpx;position: fixed;bottom: 34rpx;left: 0;right: 0;margin: 0 auto;}
