/* pages/user/user.wxss */
.user-part1{background-color: #FF6248;}
.user-info{color: #fff;padding: 38rpx 0 54rpx;}
.info-img{width: 104rpx;height: 104rpx;background-position: center;background-repeat: no-repeat;background-size: cover;margin-left: 20rpx;margin-right: 34rpx;}
.fl{float: left;}
.info-name{font-size: 32rpx;line-height: 44rpx;margin-top: 12rpx;}
.info-mobile{font-size: 28rpx;line-height: 40rpx;}
.orders{background-color: #fff;padding: 28rpx 0;border-bottom: 1px solid #F1F1F1;border-top-left-radius: 6rpx;border-top-right-radius: 6rpx;}
.order-txt{font-size: 32rpx;line-height: 44rpx;float: left;padding-left: 22rpx;}
.order-btn{font-size: 26rpx;line-height: 44rpx;float: right;position: relative;padding-right: 40rpx;}
.order-btn::after{content: '';display: block;position: absolute;top: 50%;margin-top: -11rpx;right: 14rpx;background-image: url(data:image/png;base64,);width: 12rpx;height: 22rpx;background-size: cover;background-position: center;background-repeat: no-repeat;}
.user-part2{background-color: #F5F5F5;padding-bottom: 20rpx;}
.order-list{background-color: #fff;border-bottom-left-radius: 6rpx;border-bottom-right-radius: 6rpx;display: flex;justify-content:space-around;padding: 32rpx 0;}
.list-items{text-align: center;font-size: 28rpx;line-height: 40rpx;position: relative;}
.items-img{width: 58rpx;height: 58rpx;background-size: cover;background-position: center;background-repeat: no-repeat;margin: 0 auto;}
.items-number{position: absolute;right: -4rpx;top: -6rpx;background-color: #FF6248;color: #fff;line-height: 32rpx;border-radius: 14rpx;min-width: 32rpx;text-align: center;padding: 0 10rpx;box-sizing: border-box;font-size: 20rpx;}
.items-txt{margin-top: 6rpx;}
.user-part3{background-color: #fff;}
.select-items{border-top: 1px solid #F1F1F1;font-size: 28rpx;line-height: 40rpx;padding: 42rpx 0;background-image: url(data:image/png;base64,);background-size: 14rpx 28rpx;background-repeat: no-repeat;background-position: right center;}
.select-items .txt{float: left;}
.select-items .info{float: right;}
.select-items.select-none{background: none;border-top: none;}