/* pages/waitingPay/waitingPay.wxss */
.waitingPay{height: 100vh;background-color: #fff;}
.wait-part1{padding: 30rpx 0;border-bottom: 20rpx solid #F5F5F5;background-color: #fff;font-size: 28rpx;}
.wait-part1 .txt{line-height: 40rpx;text-align: center;}
.remain-time{padding: 20rpx 0;text-align: center;}
.time-box{display: inline-block;width: 36rpx;height: 56rpx;line-height: 56rpx;color: #FF6248;border: 1px solid #FF6248;border-radius: 4rpx;margin: 0 4rpx;background-color: #FFF7F5;}
.mh{width: 30rpx;color: #FF6248;display: inline-block;font-weight: bold;}
.wait-part2{background-color: #fff;font-size: 26rpx;border-bottom: 20rpx solid #F5F5F5;padding: 20rpx 0;}
.wait-part2 .list{line-height: 26rpx;padding: 32rpx 0;}
.wait-part2 .list.prime-list{border-top: 1px solid #F0F0F0;}
.wait-part2 .list .attr{float: left;}
.wait-part2 .list .cont{float: right;}
.wait-part2 .list.prime-list .cont{color: #FF6248;}
.wait-part3{background-color: #fff;}
.wait-part3 .title{font-size: 34rpx;color: #4A4A4A ;line-height: 48rpx;padding: 40rpx 0 30rpx;}
.payment-method{border-bottom: 1px solid #F0F0F0;}
.method-list{line-height: 36rpx;padding: 20rpx 0;}
.method-name{float: left;padding-left: 80rpx;background-size: 60rpx 60rpx;background-position: left center;background-repeat: no-repeat;height: 60rpx;line-height: 60rpx;}
.select-btn{float: right;height: 60rpx;min-width: 30rpx;line-height: 60rpx;background-image: url(data:image/png;base64,);background-size: 30rpx;background-position: right center;background-repeat: no-repeat;}
.method-list.active .select-btn{background-image: url(data:image/png;base64,);}
.pay-btn{font-size: 32rpx;line-height: 98rpx;height: 98rpx;background-image: linear-gradient(90deg,  #FF9174 0%,  #FF6248 100%);color: #fff;position: fixed;bottom: 0;width: 100%;text-align: center;left: 0;}