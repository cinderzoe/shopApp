/* pages/addAddr/addAddr.wxss */
.noaddress-layOut{background-color: #fff;}
.noaddress-layIn .add-btn{font-size: 34rpx;color: #FF6248;width: 218rpx;line-height: 68rpx;opacity: 0.96;border: 1px solid #FF6248;border-radius: 34rpx;margin: 66rpx auto;text-align: center;}

.hasaddress-layout{background-color: #fff;padding: 40rpx 0;}
.addr-info{font-size: 28rpx;color: #333333;line-height: 40rpx;padding: 0 22rpx 0 56rpx;}
.addr-info .row-in{width: 126rpx;float: left;}
.addr-info .row-name,.addr-info .row-mobile{display: inline-block;vertical-align: top;}
.addr-info .row-name .row-in{letter-spacing: 9rpx;}
.addr-info .input-info-name{min-width: 90rpx;float: left;padding-right: 50rpx;}
.addr-info .input-info-address{float: left;width: 470rpx;}
.addr-info .row-address{position: relative;margin-top: 20rpx;}
.addr-info .row-address::before{content: '';display: block;width: 33rpx;height: 40rpx;background-image: url(data:image/png;base64,);background-position: center;background-size: 100% 100%;background-repeat: no-repeat;top: 0;left: -56rpx;position: absolute;}
.addr-info .row-address::after{content: '';display: block;background-image: url(data:image/png;base64,);width: 12rpx;height: 24rpx;background-size: 100% 100%;background-position: center;background-repeat: no-repeat;position: absolute;top: 10rpx;right: 0;margin-right: -20rpx;visibility: visible;}